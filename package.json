{
  "name": "sushi-fusion",
  "version": "0.2.0",
  "description": "Fast, dynamic and responsive website for a restaurant",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "test": "jest --coverage -w =100%",
    "lint": "eslint src/ --cache -f table",
    "lint-fix": "eslint src/ --fix",
    "deploy": "surge ./dist plaza-athenee.surge.sh"
  },
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^5.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "terser-webpack-plugin": "^2.3.5",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Israel-Laguan/SUSHI-FUSION.git"
  },
  "keywords": [
    "javascript",
    "js",
    "app",
    "eslint",
    "ES6",
    "Test",
    "Jest"
  ],
  "author": "Israel Laguan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Israel-Laguan/SUSHI-FUSION/issues"
  },
  "homepage": "https://github.com/Israel-Laguan/SUSHI-FUSION#readme",
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^29.7.0",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
